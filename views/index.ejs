<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Local Cloud Storage</title>
  </head>
  <body>
    <h1>Local Cloud Storage</h1>
    <form action="/upload" method="post" enctype="multipart/form-data">
      <input type="file" name="file">
      <button type="submit">Upload</button>
    </form>
    <h2>Uploaded Files:</h2>
    <ul>
      <% files.forEach(file => { %>
        <li><%= file %></li>
      <% }); %>
    </ul>
  </body>
</html>
